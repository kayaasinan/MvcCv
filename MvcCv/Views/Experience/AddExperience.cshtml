
@{
    ViewBag.Title = "AddExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<br />
<form method="post">
    <input type="text" name="Title" class="form-control" placeholder="Başlığı giriniz"  />
    <br />
    <input type="text" name="SubHead" class="form-control" placeholder="Alt başlığı giriniz" required />
    <br />
    <input type="text" name="Date" class="form-control" placeholder="Çalışma tarihi giriniz" required/>
    <br />
    <textarea class="form-control" name="Description" placeholder="Açıklamanızı giriniz" rows="5" required></textarea>
    <br />
    <button class="btn btn-success">Kaydet</button>
</form>

