@using MvcCv.Models.Entity
@model List<TblCertificates>

<div class="resume-section-content">
    <h2 class="mb-5">Sertifikalar</h2>
    @foreach (var y in Model)
    {
        <ul class="fa-ul mb-0">
            <li>
                <span class="fa-li"><i class="fas fa-trophy text-warning"></i></span>
                @y.Description
            </li>
        </ul>
    }
</div>
