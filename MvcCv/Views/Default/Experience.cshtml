@using MvcCv.Models.Entity
@model List<TblExperiences>
<div class="resume-section-content">

    <h2 class="mb-5">Deneyimlerim</h2>
    @foreach (var y in Model)
    {
        <div class="d-flex flex-column flex-md-row justify-content-between mb-5">
            <div class="flex-grow-1">
                <h3 class="mb-0">@y.Title</h3>
                <div class="subheading mb-3">@y.SubHead</div>
                <p>@y.Description</p>
            </div>
            <div class="flex-shrink-0"><span class="text-primary">@y.Date</span></div>
        </div>
    }
</div>
